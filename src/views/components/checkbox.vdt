<div class={{ "c-checkbox " + (self.get('checked') ? " checked " : "") + (self.get('className') ? self.get('className') : '') }}>
    <label>
        <input type="checkbox" checked={{ self.get('checked') }} ev-change={{ _.bind(self._change, self) }}/>
        {{ self.get('children') }}
    </label>
</div>
