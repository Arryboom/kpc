.c-tab
    background #f5f5f5
    height 45px
    line-height 45px
    border-bottom 1px solid #e6e6e6
    .tab-item
        float left
        font-size 16px
        width 13%
        text-align center
        border-right 1px solid #e6e6e6
        a
            display block
            color #78829d
    .active
        background #fff
        border-top 2px solid #218ee9
        height 44px
        line-height 42px
        a
            color #218ee9 
