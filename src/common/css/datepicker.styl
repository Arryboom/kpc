.date-show .ui-widget-content {
    border: 1px solid #dddddd;
    background: #ffffff;
    color: #333333;
}
.date-show .ui-state-default,
.date-show .ui-widget-content .ui-state-default,
.date-show .ui-widget-header .ui-state-default {
    /*border: 1px solid #c5c5c5;*/
    /*background: #f6f6f6;*/
    font-weight: normal;
    // color: #454545;
}
.date-show .ui-widget {
    font-family: Arial,Helvetica,sans-serif;
    font-size: 1em;
}
.date-show .ui-datepicker {
    width: 17em;
    padding: .2em .2em 0;
    display: none;
}
.date-show .ui-datepicker .ui-datepicker-header {
    position: relative;
    padding: .2em 0;
}
.date-show .ui-datepicker .ui-datepicker-prev,
.date-show .ui-datepicker .ui-datepicker-next {
    position: absolute;
    top: 2px;
    width: 1.8em;
    height: 1.8em;
}
.date-show .ui-datepicker .ui-datepicker-prev-hover,
.date-show .ui-datepicker .ui-datepicker-next-hover {
    top: 1px;
}
.date-show .ui-datepicker .ui-datepicker-prev {
    background-image: url("/node_modules/kpc/src/common/images/back.png");
    background-repeat:no-repeat;
    background-position: center;
    left: 2px;
}
.date-show .ui-datepicker .ui-datepicker-next {
    background-image: url("/node_modules/kpc/src/common/images/next.png");
    background-repeat:no-repeat;
    background-position: center;
    right: 2px;
}
.date-show .ui-datepicker .ui-datepicker-prev-hover {
    left: 1px;
}
.date-show .ui-datepicker .ui-datepicker-next-hover {
    right: 1px;
}
.date-show .ui-datepicker .ui-datepicker-prev span,
.date-show .ui-datepicker .ui-datepicker-next span {
    display: block;
    position: absolute;
    left: 50%;
    margin-left: -8px;
    top: 50%;
    margin-top: -8px;
}
.date-show .ui-datepicker .ui-datepicker-title {
    margin: 0 2.3em;
    line-height: 1.8em;
    text-align: center;
}
.date-show .ui-datepicker .ui-datepicker-title select {
    font-size: 1em;
    margin: 1px 0;
}
.date-show .ui-datepicker select.ui-datepicker-month,
.date-show .ui-datepicker select.ui-datepicker-year {
    width: 45%;
}
.date-show .ui-datepicker table {
    width: 100%;
    font-size: .9em;
    border-collapse: collapse;
    margin: 0 0 .4em;
}
.date-show .ui-datepicker th {
    padding: .7em .3em;
    text-align: center;
    //font-weight: bold;
    border: 0;
}
.date-show .ui-datepicker td {
    border: 0;
    padding: 1px;
}
.date-show .ui-datepicker td span,
.date-show .ui-datepicker td a {
    display: block;
    padding: .2em;
    text-align: center;
    text-decoration: none;
}
.date-show .ui-datepicker .ui-datepicker-buttonpane {
    background-image: none;
    margin: .7em 0 0 0;
    padding: 0 .2em;
    border-left: 0;
    border-right: 0;
    border-bottom: 0;
}
.date-show .ui-datepicker .ui-datepicker-buttonpane button {
    float: right;
    margin: .5em .2em .4em;
    cursor: pointer;
    padding: .2em .6em .3em .6em;
    width: auto;
    overflow: visible;
}
.date-show .ui-datepicker .ui-datepicker-buttonpane button.ui-datepicker-current {
    float: left;
}

/* with multiple calendars */
.date-show .ui-datepicker.ui-datepicker-multi {
    width: auto;
}
.date-show .ui-datepicker-multi .ui-datepicker-group {
    float: left;
}
.date-show .ui-datepicker-multi .ui-datepicker-group table {
    width: 95%;
    margin: 0 auto .4em;
}
.date-show .ui-datepicker-multi-2 .ui-datepicker-group {
    width: 50%;
}
.date-show .ui-datepicker-multi-3 .ui-datepicker-group {
    width: 33.3%;
}
.date-show .ui-datepicker-multi-4 .ui-datepicker-group {
    width: 25%;
}
.date-show .ui-datepicker-multi .ui-datepicker-group-last .ui-datepicker-header,
.date-show .ui-datepicker-multi .ui-datepicker-group-middle .ui-datepicker-header {
    border-left-width: 0;
}
.date-show .ui-datepicker-multi .ui-datepicker-buttonpane {
    clear: left;
}
.date-show .ui-datepicker-row-break {
    clear: both;
    width: 100%;
    font-size: 0;
}

/* RTL support */
.date-show .ui-datepicker-rtl {
    direction: rtl;
}
.date-show .ui-datepicker-rtl .ui-datepicker-prev {
    right: 2px;
    left: auto;
}
.date-show .ui-datepicker-rtl .ui-datepicker-next {
    left: 2px;
    right: auto;
}
.date-show .ui-datepicker-rtl .ui-datepicker-prev:hover {
    right: 1px;
    left: auto;
}
.date-show .ui-datepicker-rtl .ui-datepicker-next:hover {
    left: 1px;
    right: auto;
}
.date-show .ui-datepicker-rtl .ui-datepicker-buttonpane {
    clear: right;
}
.date-show .ui-datepicker-rtl .ui-datepicker-buttonpane button {
    float: left;
}
.date-show .ui-datepicker-rtl .ui-datepicker-buttonpane button.ui-datepicker-current,
.date-show .ui-datepicker-rtl .ui-datepicker-group {
    float: right;
}
.date-show .ui-datepicker-rtl .ui-datepicker-group-last .ui-datepicker-header,
.date-show .ui-datepicker-rtl .ui-datepicker-group-middle .ui-datepicker-header {
    border-right-width: 0;
    border-left-width: 1px;
}
