            "content": "import {Tabs, Tab} from 'components/tabs';\n\n<div>\n    <Tabs v-model=\"tab3\" style=\"margin: 20px;width:150px\" vertical>\n        <Tab value=\"rulein\">入站规则</Tab>\n        <Tab value=\"ruleout\">出站规则</Tab>\n        <Tab value=\"relatedVM\">关联云主机</Tab>\n    </Tabs>\n</div>\n"
